{
  "name": "@goldwasserexchange/webpack-server",
  "version": "6.1.1",
  "description": "The webpack server preconfigured with goldwasserexchange defaults",
  "homepage": "https://github.com/goldwasserexchange/public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldwasserexchange/public.git"
  },
  "license": "MIT",
  "author": "Hans Scheuren",
  "main": "index.js",
  "scripts": {
    "lint": "yarn lint:pkg && yarn lint:js",
    "lint:js": "ge-scripts lint-js",
    "lint:pkg": "npmPkgJsonLint .",
    "precommit": "lint-staged",
    "sort": "sort-package-json"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:js"
    ],
    "package.json": [
      "npmPkgJsonLint"
    ]
  },
  "eslintConfig": {
    "extends": "@goldwasserexchange"
  },
  "dependencies": {
    "@goldwasserexchange/read-pkg-up-helpers": "^6.0.1",
    "@goldwasserexchange/webpack-config": "^6.1.1",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "minimist": "^1.2.0",
    "react-dev-utils": "^8.0.0",
    "webpack": "^4.41.6",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "@goldwasserexchange/eslint-config": "^6.1.1",
    "@goldwasserexchange/npm-package-json-lint-config": "^6.0.1",
    "ge-scripts": "^6.1.1",
    "lint-staged": "^10.0.7",
    "npm-package-json-lint": "^4.6.0",
    "sort-package-json": "^1.39.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmpackagejsonlint": {
    "extends": "@goldwasserexchange/npm-package-json-lint-config"
  }
}
