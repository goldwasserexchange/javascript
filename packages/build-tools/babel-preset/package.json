{
  "name": "@goldwasserexchange/babel-preset",
  "version": "5.5.2",
  "description": "The babel preset for Goldwasser Exchange projects",
  "homepage": "https://github.com/goldwasserexchange/public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldwasserexchange/public.git"
  },
  "license": "MIT",
  "author": "Hans Scheuren",
  "main": "index.js",
  "scripts": {
    "lint": "yarn lint:pkg && yarn lint:js",
    "lint:js": "ge-scripts lint-js",
    "lint:pkg": "npmPkgJsonLint .",
    "precommit": "lint-staged",
    "sort": "sort-package-json"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:js"
    ],
    "package.json": [
      "npmPkgJsonLint"
    ]
  },
  "eslintConfig": {
    "extends": "@goldwasserexchange"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-exponentiation-operator": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-react-jsx-self": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "^7.0.0",
    "@goldwasserexchange/browserslist": "^5.5.2",
    "@goldwasserexchange/node-semver-tools": "^5.5.2",
    "@goldwasserexchange/read-pkg-up-helpers": "^5.5.2",
    "babel-plugin-date-fns": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "babel-plugin-ramda": "^2.0.0",
    "babel-plugin-recharts": "^1.1.1",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-universal-import": "^3.0.0",
    "ramda": "^0.26.1",
    "react-loadable": "^5.3.1"
  },
  "devDependencies": {
    "@goldwasserexchange/eslint-config": "^5.5.2",
    "@goldwasserexchange/npm-package-json-lint-config": "^5.5.2",
    "ge-scripts": "^5.5.2",
    "lint-staged": "^8.1.0",
    "npm-package-json-lint": "^3.4.1",
    "sort-package-json": "^1.22.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmPackageJsonLintConfig": {
    "extends": "@goldwasserexchange/npm-package-json-lint-config"
  }
}
