    "@goldwasserexchange/eslint-config": "^6.1.0",
    "@goldwasserexchange/npm-package-json-lint-config": "^6.0.1",
    "ge-scripts": "^6.1.0",
    "lint-staged": "^9.2.5",
    "npm-package-json-lint": "^3.4.1",
    "sort-package-json": "^1.22.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmPackageJsonLintConfig": {
    "extends": "@goldwasserexchange/npm-package-json-lint-config"
  }
