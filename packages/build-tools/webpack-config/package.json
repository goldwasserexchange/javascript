    "@goldwasserexchange/read-pkg-up-helpers": "^6.0.1",
    "babel-loader": "^8.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "copy-webpack-plugin": "^5.0.3",
