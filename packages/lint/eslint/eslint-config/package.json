{
  "name": "@goldwasserexchange/eslint-config",
  "version": "5.4.0",
  "description": "Goldwasser Exchange ES Lint config",
  "homepage": "https://github.com/goldwasserexchange/public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldwasserexchange/public.git"
  },
  "license": "MIT",
  "author": "Hans Scheuren",
  "main": "index.js",
  "scripts": {
    "lint": "yarn lint:pkg && yarn lint:js",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:pkg": "npmPkgJsonLint .",
    "precommit": "lint-staged",
    "sort": "sort-package-json"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:js"
    ],
    "package.json": [
      "npmPkgJsonLint"
    ]
  },
  "eslintConfig": {
    "extends": "./index.js"
  },
  "dependencies": {
    "@goldwasserexchange/eslint-config-jest": "^5.4.0",
    "@goldwasserexchange/eslint-config-ramda": "^5.4.0",
    "@goldwasserexchange/eslint-config-react": "^5.4.0",
    "@goldwasserexchange/eslint-config-strict": "^5.4.0",
    "@goldwasserexchange/eslint-config-unicorn": "^5.4.0",
    "@goldwasserexchange/read-pkg-up-helpers": "^5.0.0",
    "@typescript-eslint/parser": "^1.3.0",
    "arrify": "^2.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-redux-saga": "^0.10.0",
    "globby": "^9.0.0",
    "ramda": "^0.26.1",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@goldwasserexchange/npm-package-json-lint-config": "^5.4.0",
    "eslint": "^5.5.0",
    "lint-staged": "^8.1.0",
    "npm-package-json-lint": "^3.4.1",
    "sort-package-json": "^1.22.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmPackageJsonLintConfig": {
    "extends": "@goldwasserexchange/npm-package-json-lint-config"
  }
}
