{
  "name": "@goldwasserexchange/eslint-config",
  "version": "6.2.4",
  "description": "Goldwasser Exchange ES Lint config",
  "homepage": "https://github.com/goldwasserexchange/public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldwasserexchange/public.git"
  },
  "license": "MIT",
  "author": "Hans Scheuren",
  "main": "index.js",
  "scripts": {
    "lint": "yarn lint:pkg && yarn lint:js",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:pkg": "npmPkgJsonLint .",
    "precommit": "lint-staged",
    "sort": "sort-package-json"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint:js"
    ],
    "package.json": [
      "npmPkgJsonLint"
    ]
  },
  "eslintConfig": {
    "extends": "./index.js"
  },
  "dependencies": {
    "@goldwasserexchange/eslint-config-jest": "^6.2.3",
    "@goldwasserexchange/eslint-config-ramda": "^6.2.3",
    "@goldwasserexchange/eslint-config-react": "^6.2.3",
    "@goldwasserexchange/eslint-config-strict": "^6.2.4",
    "@goldwasserexchange/eslint-config-typescript": "^6.2.3",
    "@goldwasserexchange/eslint-config-unicorn": "^6.2.3",
    "@goldwasserexchange/read-pkg-up-helpers": "^6.2.4",
    "@typescript-eslint/parser": "^2.19.2",
    "arrify": "^2.0.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-redux-saga": "^0.10.0",
    "ramda": "^0.27.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "@goldwasserexchange/npm-package-json-lint-config": "^6.2.3",
    "eslint": "^6.8.0",
    "lint-staged": "^10.0.10",
    "npm-package-json-lint": "^4.6.0",
    "sort-package-json": "^1.39.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "npmpackagejsonlint": {
    "extends": "@goldwasserexchange/npm-package-json-lint-config"
  }
}
