{
  "name": "@goldwasserexchange/utils",
  "version": "1.8.1",
  "description": "GE Utils",
  "author": "Jonathan Goldwasser <jonathan@goldwasserexchange.be>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/goldwasserexchange/ge-utils"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=test ge-scripts lint-js",
    "test": "cross-env NODE_ENV=test BABEL_ES_TARGET=commonjs jest --coverage",
    "generate-index": "cross-env NODE_ENV=production BABEL_ES_TARGET=commonjs babel-node ./scripts/generateIndex",
    "clean": "rimraf lib es",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ES_TARGET=commonjs babel src --out-dir lib",
    "build:es": "cross-env NODE_ENV=production BABEL_ES_TARGET=es babel src --out-dir es",
    "build": "npm run clean && npm run generate-index && npm run build:commonjs && npm run build:es"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "eslintConfig": {
    "extends": "@goldwasserexchange"
  },
  "babel": {
    "presets": [
      "@goldwasserexchange/babel-preset"
    ],
    "plugins": [
      "annotate-pure-calls"
    ]
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@goldwasserexchange/babel-preset": "^2.0.4",
    "@goldwasserexchange/lint-js": "^1.9.5",
    "babel-cli": "^6.26.0",
    "babel-plugin-annotate-pure-calls": "^0.2.0",
    "cross-env": "^5.1.0",
    "ge-scripts": "3.3.0",
    "jest": "^21.2.1",
    "rimraf": "^2.6.2"
  }
}
