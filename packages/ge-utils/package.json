{
  "name": "@goldwasserexchange/utils",
  "version": "1.2.0",
  "description": "GE Utils",
  "author": "Jonathan Goldwasser <jonathan@goldwasserexchange.be>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/goldwasserexchange/ge-utils"
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "test": "cross-env BABEL_ENV=commonjs jest --coverage",
    "generate-index": "cross-env BABEL_ENV=commonjs babel-node ./scripts/generateIndex",
    "clean": "rimraf lib es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build": "npm run clean && npm run generate-index && npm run build:commonjs && npm run build:es"
  },
  "main": "lib/index.js",
  "modules": "es/index.js",
  "jsnext:main": "es/index.js",
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "linebreak-style": 0
    }
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "ramda"
    ],
    "env": {
      "commonjs": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "loose": true
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-plugin-ramda": "^1.2.0",
    "babel-preset-es2015": "^6.24.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "rimraf": "^2.6.1"
  }
}
