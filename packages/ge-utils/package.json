{
  "name": "@goldwasserexchange/utils",
  "version": "0.3.0",
  "description": "GE Utils",
  "author": "Jonathan Goldwasser <jonathan@goldwasserexchange.be>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/goldwasserexchange/ge-utils"
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "test": "cross-env BABEL_ENV=test nyc ava",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:publish": "babel src --out-dir ."
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "linebreak-style": 0
    }
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "ramda"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "loose": true
            }
          ]
        ]
      }
    }
  },
  "ava": {
    "require": "babel-register"
  },
  "dependencies": {
    "json-stringify-safe": "5.0.1",
    "ramda": "0.23.0"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-plugin-ramda": "1.1.9",
    "babel-preset-es2015": "6.22.0",
    "babel-register": "6.23.0",
    "cross-env": "3.2.3",
    "eslint": "3.17.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "nyc": "10.1.2",
    "rimraf": "2.6.1"
  }
}
