{
  "name": "@goldwasserexchange/utils",
  "version": "1.12.0",
  "description": "GE Utils",
  "author": "Jonathan Goldwasser <jonathan@goldwasserexchange.be>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/goldwasserexchange/ge-utils"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=test ge-scripts lint-js",
    "test": "cross-env NODE_ENV=test BABEL_ES_TARGET=commonjs jest --coverage --collectCoverageFrom **/src/*.js --collectCoverageFrom !**/src/index.js",
    "generate-index": "cross-env NODE_ENV=production BABEL_ES_TARGET=commonjs babel-node ./scripts/generateIndex",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ES_TARGET=commonjs ge-scripts babel",
    "build:es": "cross-env NODE_ENV=production BABEL_ES_TARGET=es ge-scripts babel",
    "build": "npm run generate-index && npm run build:commonjs && npm run build:es"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "eslintConfig": {
    "extends": "@goldwasserexchange"
  },
  "babel": {
    "presets": [
      "@goldwasserexchange/babel-preset"
    ],
    "plugins": [
      "annotate-pure-calls"
    ]
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-plugin-annotate-pure-calls": "^0.2.0",
    "cross-env": "^5.1.0",
    "ge-scripts": "3.21.1",
    "jest": "^21.2.1"
  }
}
