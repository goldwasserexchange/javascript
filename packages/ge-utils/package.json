{
  "name": "@goldwasserexchange/utils",
  "version": "0.3.2",
  "description": "GE Utils",
  "author": "Jonathan Goldwasser <jonathan@goldwasserexchange.be>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/goldwasserexchange/ge-utils"
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "test": "jest --coverage",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:publish": "babel src --out-dir ."
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "linebreak-style": 0
    }
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "ramda"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "loose": true
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "json-stringify-safe": "5.0.1",
    "ramda": "0.23.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-plugin-ramda": "1.2.0",
    "babel-preset-es2015": "6.24.0",
    "eslint": "3.18.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "jest": "19.0.2",
    "rimraf": "2.6.1"
  }
}
